      "isAuthEntity": true,
      "requiresAuth": true,
      "graphql": true
    },
    "roles": {
      "name": "roles",
      "nameSingular": "role",
      "subFolder": "",
      "fileName": "roles",
      "adapter": "sequelize",
      "path": "/roles",
      "isAuthEntity": false,
      "requiresAuth": true,
      "graphql": true
    },
    "teams": {
      "name": "teams",
      "nameSingular": "team",
      "subFolder": "",
      "fileName": "teams",
