{
  "options": {
    "ver": "1.0.0",
    "inspectConflicts": false,
    "semicolons": true,
    "freeze": [],
    "ts": false
  },
  "app": {
    "environmentsAllowingSeedData": "test",
    "seedData": false,
    "name": "rescueouranimals-server",
    "description": "Project rescueouranimals-server",
    "src": "src",
    "packager": "yarn@>= 0.18.0",
